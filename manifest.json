{
    "name": "YouTube Cat",
    "version": "1.0",
    "description": "Category blocker/filter for YouTube",
    "background": {
        "scripts": [ "background.js" ]
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ],
    "manifest_version": 2
}

